{
	"extends": [
		"standard",
		"standard-react"
	],
	"parser": "babel-eslint",
	"rules": {
		"semi": [
			2,
			"always"
		],
		"react/jsx-no-bind": [
			1
		],
		"react/prop-types": [
			1
		],
		"no-undef": [
			1
		],
		"handle-callback-err": [
			1
		],
		"no-new": [
			1
		],
		"no-fallthrough": [
			1
		]
	}
}