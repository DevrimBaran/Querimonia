{
	"extends": [
		"standard",
		"standard-react"
	],
	"parser": "babel-eslint",
	"globals": {
		"fetch": false,
		"Blob": false,
		"Response": false,
		"FormData": false,
		"d3": false,
		"btoa": false
	},
	"rules": {
		"semi": [
			2,
			"always"
		],
		"react/jsx-no-bind": [
			1
		],
		"react/prop-types": [
			0
		],
		"no-undef": [
			0
		],
		"handle-callback-err": [
			1
		],
		"no-new": [
			1
		],
		"no-fallthrough": [
			1
		],
		"no-return-assign": [
			0
		]
	}
}
